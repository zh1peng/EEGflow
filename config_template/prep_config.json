{
  "steps": [
    {
      "id": "S001",
      "name": "Load",
      "op": "load_set",
      "args": {
        "filename": "",
        "filepath": ""
      }
    },
    {
      "id": "S003",
      "name": "Downsample",
      "op": "downsample",
      "args": {
        "Rate": 250
      }
    },
    {
      "id": "S002",
      "name": "Filter",
      "op": "filter",
      "args": {
        "LowCutoff": 0.5,
        "HighCutoff": 30
      }
    },
    {
      "id": "S004",
      "name": "Crop",
      "op": "crop_by_markers",
      "args": {
        "StartMarker": "98",
        "EndMarker": "99",
        "PadSec": 0.2
      }
    },
    {
      "id": "S005",
      "name": "BadChannels",
      "op": "remove_bad_channels",
      "args": {
        "ExcludeLabel": ["VEOU", "VEOL", "HEOR", "HEOL", "ECG"],
        "Action": "remove",
        "Kurtosis": true,
        "Kurt_Threshold": 5,
        "Probability": false,
        "Prob_Threshold": 5,
        "Spectrum": false,
        "Spec_Threshold": 5,
        "Spec_FreqRange": [1, 50],
        "NormOn": "on",
        "FASTER_MeanCorr": false,
        "FASTER_Threshold": 0.4,
        "FASTER_RefChan": [],
        "FASTER_Bandpass": [],
        "FASTER_Variance": false,
        "FASTER_VarThreshold": 3,
        "FASTER_Hurst": false,
        "FASTER_HurstThreshold": 3,
        "CleanRaw_Flatline": false,
        "Flatline_Sec": 5,
        "CleanDrift_Band": [0.25, 0.75],
        "CleanRaw_Noise": false,
        "CleanChan_Corr": 0.8,
        "CleanChan_Line": 4,
        "CleanChan_MaxBad": 0.5,
        "CleanChan_NSamp": 50
      }
    },
    {
      "id": "S006",
      "name": "RemoveChannels",
      "op": "remove_channels",
      "args": {
        "Chan2remove": ["ECG"]
      }
    },
    {
      "id": "S007",
      "name": "Reref",
      "op": "reref",
      "args": {
        "ExcludeLabel": []
      }
    },
    {
      "id": "S008",
      "name": "Save",
      "op": "save_set",
      "args": {
        "filename": "",
        "filepath": ""
      }
    }
  ],
  "Options": {
    "validate_only": false
  }
}
